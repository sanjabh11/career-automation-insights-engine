# Supabase Configuration
# Get these from: https://supabase.com/dashboard/project/YOUR_PROJECT/settings/api
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your_anon_key_here
VITE_SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here

# Gemini AI
# Get API key from: https://aistudio.google.com/app/apikey
VITE_GEMINI_API_KEY=your_gemini_api_key_here
GEMINI_API_KEY=your_gemini_api_key_here
GEMINI_MODEL=gemini-2.5-flash

# SerpAPI (Job Search)
# Get API key from: https://serpapi.com/manage-api-key
VITE_SERPAPI_API_KEY=your_serpapi_key_here
# Used by Edge Functions (skill-demand-scraper)
SERPAPI_API_KEY=your_serpapi_key_here
SERP_API_KEY=your_serpapi_key_here

# Bureau of Labor Statistics API (optional)
BLS_API_KEY=your_bls_api_key_here

# O*NET Web Services
# Register at: https://services.onetcenter.org/developer/signup
ONET_USERNAME=your_onet_username
ONET_PASSWORD=your_onet_password

# APO Function Security (Optional)
# Generate with: openssl rand -hex 32
APO_FUNCTION_API_KEY=generate_random_32_char_hex_string_here
VITE_APO_FUNCTION_API_KEY=same_as_above

# Economics importer function key (required for /econ-importer)
VITE_ECON_SYNC_API_KEY=your_econ_sync_api_key_here

# Functions base URL for local development
# Leave commented out when using Supabase Edge Functions
# VITE_FUNCTIONS_BASE=http://localhost:8888

# Optional per-minute rate limits for PH6 functions
# Defaults used if not set
HL_RATE_LIMIT_PER_MIN=60
AR_RATE_LIMIT_PER_MIN=60
SIM_RATE_LIMIT_PER_MIN=30
CASCADE_RATE_LIMIT_PER_MIN=30
PORTFOLIO_RATE_LIMIT_PER_MIN=30

# Whop Integration (Membership & Payments)
# Get these from: https://whop.com/apps (Create a new app)
WHOP_API_KEY=your_whop_api_key_here
WHOP_WEBHOOK_SECRET=your_whop_webhook_secret_here
# Company ID for frontend (visible in Whop dashboard URL)
VITE_WHOP_COMPANY_ID=your_whop_company_id_here
# App ID for OAuth (if implementing OAuth flow)
VITE_WHOP_APP_ID=your_whop_app_id_here
